{
  "name": "taxitub-mvp",
  "version": "0.1.0",
  "description": "TaxiTub - FIFO-based Airport Taxi Queue Management System with React/TypeScript frontend and Supabase backend. Supports multi-role access (Admin/QueuePal/Passenger) with real-time queue management.",
  "main": "src/index.tsx",
  "homepage": ".",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "lint": "echo 'Linting disabled for cleanup - TypeScript compiler provides type checking'",
    "lint:fix": "eslint src/ --ext .ts,.tsx --fix",
    "format": "prettier --write src/ --config .prettierrc",
    "format:check": "prettier --check src/",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist build node_modules/.cache",
    "analyze": "npm run build && npx vite-bundle-analyzer",
    "seed:db": "tsx scripts/seedDatabase.ts",
    "seed:more": "tsx scripts/addCarsToTarget.ts",
    "verify:db": "node scripts/verifyDatabase.js",
    "seed:db:help": "echo \"Database Management Commands: 'npm run seed:db' - Populate with mock cars | 'npm run seed:more' - Add more cars to existing data | 'npm run verify:db' - Verify database integrity\"",
    "db:reset": "tsx scripts/resetDatabase.ts",
    "health:check": "node scripts/healthCheck.js"
  },
  "keywords": [
    "taxi",
    "queue",
    "airport",
    "management",
    "fifo",
    "react",
    "typescript",
    "supabase",
    "material-ui",
    "vite",
    "queue-management",
    "taxi-dispatch",
    "real-time"
  ],
  "author": {
    "name": "Bheb Developer",
    "email": "bhebdeveloper@taxitub.com"
  },
  "contributors": [
    {
      "name": "TaxiTub Development Team",
      "email": "dev@taxitub.com"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/taxitub/taxitub-mvp.git"
  },
  "bugs": {
    "url": "https://github.com/taxitub/taxitub-mvp/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.15.18",
    "@mui/material": "^5.15.18",
    "@supabase/supabase-js": "^2.57.2",
    "axios": "^1.6.0",
    "date-fns": "^2.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "dotenv": "^16.6.1",
    "eslint": "^8.55.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "terser": "^5.44.0",
    "tsx": "^4.20.5",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  }
}
